angle = :0.5:80;
speed = 40:0.5:80;
numAngle = length(angle);
numSpeed = length(speed);
height = zeros(numAngle, numSpeed);
for i=1:numSpeed
    for j=1:numAngle
        height(i,j) = tennisball_ode45(speed(i),angle(j));
    end
end
pcolor (angle, speed, height);
colorbar;
hold on;
contour (angle, speed, height, [1 1],'r');
contour (angle, speed, height, [12 12],'r');
xlabel ('angle');
ylabel ('speed');


